#include<iostream>
using namespace std;
int main()
{
	int n1,n2;
	char op;
	cout<<"enter the operators : *,-,+,/,% = ";
	cin>>op;
	cout<<"enter the operands : ";
	cin>>n1>>n2;
	switch(op)
	{
		case'+':
			cout<<n1<<"+"<<n2<<"="<<n1+n2;
			break;
		case'-':
			cout<<n1<<"-"<<n2<<"="<<n1-n2;
			break;
		case'*':
			cout<<n1<<"*"<<n2<<"="<<n1*n2;
			break;
		case'/':
			cout<<n1<<"/"<<n2<<"="<<n1/n2;
			break;
		case'%':
			cout<<n1<<"%"<<n2<<"="<<n1%n2;
			break;
	}
	return 0;
}
